Trust model type: b # c for cumulative, b for bounded
Trust weight: 5.0 # positive float
Gossip mode: p # p for public, f for friend based
Gossip weight: 5.0 # positive float
Gossip range: 4 # integer, only applicable in friend-based gossip model
Runs: 20 # integer number of simulation runs
Steps: 400 # integer number of timesteps
Patients: 1000 # integer number of patients
Retailers: 100 # integer number of retailers
Suppliers: 10 #integer number of suppliers
Standard distance: 0.02 # float, smallest possible distance between patient and retailer
Minimum lifetime: 4 # integer, shortest possible lifetime for suppliers and retailers before going bankrupt
