Trust model type: c # c for cumulative, b for bounded
Trust weight: 0.6 # float between 0 and 1
Gossip mode: p # p for public, f for friend based
Gossip weight: 0.4 # float between 0 and 1
Gossip range: 2 # integer, only applicable in friend-based gossip model
Runs: 2 # integer number of simulation runs
Steps: 10 # integer number of timesteps
Patients: 1000 # integer number of patients
Retailers: 100 # integer number of retailers
Suppliers: 10 #integer number of suppliers
Standard distance: 1.0 # float, smallest possible distance between patient and retailer
Retailer overhead: 1 # integer, inventory removed from retailer at the end of a round
Supplier overhead: 1 # integer, inventory removed from supplier at the end of a round